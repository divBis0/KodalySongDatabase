<p id="notice"><%= notice %></p>

<h1>Songs</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Last Modified</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @songs.each do |song| %>
      <tr>
        <td><%= link_to song.title, song %></td>
        <td><%= song.updated_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= paginate @songs %>

<br>

<%= link_to 'New Song', new_song_path %>
